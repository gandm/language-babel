{
  ".source.js.jsx": {
    "editor": {
      "commentStart": "// ",
      "foldEndPattern": "^\\s*\\}|^\\s*\\]|^\\s*\\)",
      "increaseIndentPattern": "{[^}\"']*$|\\[[^\\]\"']*$|\\([^)\"']*$|<[^/]([^>]|{[^}]*})*>\\s*$",
      "decreaseIndentPattern": "^\\s*(\\s*/[*].*[*]/\\s*)*([}\\])]|</[^>]*>)"
    }
  }
}
